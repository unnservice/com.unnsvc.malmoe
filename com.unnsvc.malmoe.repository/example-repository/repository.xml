<repository xmlns="urn:malmoe:repository">

	<identity>
		<groups>
			<group name="testGroup">
				<permission id="artifact.read" />
				<permission id="artifact.write" />
				<permission id="artifact.overwrite" />
			</group>
		</groups>
		<users>
			<user username="admin" password="BVZfi2WIUnWOVHgrMQU7ZifhRzL9WiLiEybUf1cgKqY=" salt="0/8OvHBfZAkDhT6NQAqlag==">
				<group ref="testGroup" />
			</user>
		</users>
	</identity>

	<repositories>
		<proxyRepository name="main">
			<access>
				<group ref="testGroup" />
			</access>
			<proxyChain>
				<repository ref="custom" />
			</proxyChain>
		</proxyRepository>
		<!-- these will download and save artifacts into their own locations -->
		<virtualRepository name="customMaven">
			<access>
				<group ref="testGroup" />
			</access>
			<resolver name="maven" url="http://someMavenLocation/" />
		</virtualRepository>
		<virtualRepository name="customMalmoe">
			<access>
				<group ref="testGroup" />
			</access>
			<resolver name="malmoe" url="http://someMalmoeLocation/" />
		</virtualRepository>
	</repositories>

</repository>
